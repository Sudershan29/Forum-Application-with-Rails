<h1>Post List</h1>
<table>
  <tr>
    <th><%= link_to "Title", :sort => "title"%> </th>
    <th><%= link_to "Content", :sort => "content"%> </th>
    <th><%= link_to "Likes", :sort => "upvotes"%> </th>
    <th><%= link_to "Dislikes", :sort => "downvotes"%> </th>
  </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.title%></td>
    <td><%=post.content %></td><td>
    <td>  <%=post.upvotes %></td>
    <td>  <%=post.downvotes %></td>
  </tr>
  <% end %>
  </table>